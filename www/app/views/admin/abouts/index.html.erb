<h1>Listing admin_abouts</h1>

<table>
  <thead>
    <tr>
      <th>Title area 1</th>
      <th>Content area 1</th>
      <th>Title area 2</th>
      <th>Content area 2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_abouts.each do |admin_about| %>
      <tr>
        <td><%= admin_about.title_area_1 %></td>
        <td><%= admin_about.content_area_1 %></td>
        <td><%= admin_about.title_area_2 %></td>
        <td><%= admin_about.content_area_2 %></td>
        <td><%= link_to 'Show', admin_about %></td>
        <td><%= link_to 'Edit', edit_admin_about_path(admin_about) %></td>
        <td><%= link_to 'Destroy', admin_about, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New About', new_admin_about_path %>
