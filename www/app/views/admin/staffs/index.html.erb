<h1>Listing admin_staffs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Office</th>
      <th>Slogan</th>
      <th>Content</th>
      <th>About</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_staffs.each do |admin_staff| %>
      <tr>
        <td><%= admin_staff.name %></td>
        <td><%= admin_staff.office %></td>
        <td><%= admin_staff.slogan %></td>
        <td><%= admin_staff.content %></td>
        <td><%= admin_staff.about_id %></td>
        <td><%= link_to 'Show', admin_staff %></td>
        <td><%= link_to 'Edit', edit_admin_staff_path(admin_staff) %></td>
        <td><%= link_to 'Destroy', admin_staff, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Staff', new_admin_staff_path %>
